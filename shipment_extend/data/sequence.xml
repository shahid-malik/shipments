<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="seq_po_lot" model="ir.sequence">
        <field name="name">Purchase Shipment</field>
        <field name="code">po.shipment</field>
        <field name="prefix">SH</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="shipment_product" model="product.product">
        <field name="name">Shipment Charges</field>
        <field name="type">service</field>
        <field name="description">Shipment Charges</field>
        <field name="default_code">SH0001</field>
    </record>

</odoo>